{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      },
      "continue": true
    },
    {
      "src": "/api/leads/(.*)/call",
      "dest": "https://smartlead-python-six.vercel.app/leads/$1/call",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/leads/(.*)/status",
      "dest": "https://smartlead-python-six.vercel.app/leads/$1/status",
      "methods": ["PUT", "OPTIONS"]
    },
    {
      "src": "/api/leads/(.*)",
      "dest": "https://smartlead-python-six.vercel.app/leads/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/process-emails",
      "dest": "https://smartlead-python-six.vercel.app/process-emails",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.(.*)",
      "dest": "/favicon.$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}