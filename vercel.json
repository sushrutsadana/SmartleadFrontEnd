{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/leads/.*",
      "dest": "https://smartlead-python-six.vercel.app/leads/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/api/process-emails",
      "dest": "https://smartlead-python-six.vercel.app/process-emails",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.(.*)",
      "dest": "/favicon.$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}